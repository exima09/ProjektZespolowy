{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
      "whitespace": false,
      "semicolon": false,
      "no-trailing-whitespace": false,
      "no-console": [true, "log", "warning"],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
